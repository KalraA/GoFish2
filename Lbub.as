package  {		import flash.display.MovieClip;	import flash.events.*;			public class Lbub extends MovieClip{		//VARIABLES 		//this will act as the root of the document		//so we can easily reference it within the class		public var ability:String = "Lives";		public var power:Boolean = true;		public var score:int = 40;		public var enemy:Boolean = false;		public var isRight:Boolean;		public var num1:int = 33;		public var num2:int = 33;		private var _root:Object;		public var textBox:String = " ";		public var realTotal:int;		//how quickly the lbub will move		public var speed:int = Math.floor(Math.random()*10)+5;		//this function will run every time the Bullet is added		//to the stage		public function Lbub(){			//adding events to this class			//functions that will run only when the MC is added			addEventListener(Event.ADDED, beginClass);			//functions that will run on enter frame			addEventListener(Event.ENTER_FRAME, eFrame);		}		private function beginClass(event:Event):void{			_root = MovieClip(root);		}		public function Check(total:Number):void {				isRight = true;		}		public function SelfRemoval ():void {				trace("Bob");				removeEventListener(Event.ENTER_FRAME, eFrame);				this.x += 4000;				_root.removeChild(this);		}		private function eFrame(event:Event):void{			//moving the bullet up screen			y -= speed;			//making the bullet be removed if it goes off stage			if(this.x > stage.stageWidth){			SelfRemoval();			}		}		public function makeNum(total:Number,randomz:Number):String {		isRight = false;		return textBox;		}		public function goDie ():void {				}	}}