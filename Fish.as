package  {		import flash.display.MovieClip;	import flash.events.*;			public class Fish extends MovieClip{		//VARIABLES 		//this will act as the root of the document		//so we can easily reference it within the class		public var power:Boolean = false;		public var enemy:Boolean = false;		public var score:int = 350;		public var isRight:Boolean;		public var num1:int;		public var num2:int;		private var _root:Object;		public var textBox:String = " ";		public var realTotal:int;		private var levelz:int = 0;		//how quickly the fish will move		public var speed:int = Math.floor(Math.random()*5)+5;		//this function will run every time the Bullet is added		//to the stage		public function Fish(){			//adding events to this class			//functions that will run only when the MC is added			addEventListener(Event.ADDED, beginClass);			//functions that will run on enter frame			addEventListener(Event.ENTER_FRAME, eFrame);		}		private function beginClass(event:Event):void{			_root = MovieClip(root);		}		public function Check(total:Number):void {			trace(num1 + " " + num2 + "=" + total);			if (levelz == 1&&num1+num2 == total) {				isRight = true;			} else if (levelz == 2&&num1-num2 == total){				isRight = true			} else if (levelz == 3&&num1*num2 == total){				isRight = true			} else if (levelz == 4&&num1/num2 == total){				isRight = true			} else {				isRight = false;			}		}		public function SelfRemoval ():void {				trace("Bob");				removeEventListener(Event.ENTER_FRAME, eFrame);				this.x += 4000;				_root.removeChild(this);		}		private function eFrame(event:Event):void{			//moving the bullet up screen			x += speed;			//making the bullet be removed if it goes off stage			if(this.x > stage.stageWidth){			SelfRemoval();			}		}		public function makeNum(total:Number,randomz:Number, level:int):String {		randomz = Math.random()		levelz = level;						if (level == 1) {		realTotal = Math.floor(Math.random()*25);		if (randomz >  0.66) {		num1 = Math.floor(Math.random()*total);		num2 = total - num1;		trace(num1 + "+" + num2 + "=" + total);		trace("correct answer");		} else {		num1 = Math.floor(Math.random()*realTotal);		num2 = realTotal - num1;		}		textBox = num1 + "+" + num2;		}								if (level == 2) {		realTotal = Math.floor(Math.random()*25);		if (randomz >  0.66) {		num1 = Math.floor(Math.random()*(30-total)+total);		num2 = num1 - total;		} else {		num1 = Math.floor(Math.random()*(30-realTotal)+realTotal);		num2 = num1 - realTotal;		}		textBox = num1 + "-" + num2;		}						if (level == 3) {		if (randomz >  0.66) {		for (var i=0; i<10; i++) {			if (total%i == 0) {				if (total/i < 11) {					num1 = i;					i = 10;				}			}		}		num2 = total/num1;		} else {		num1 = Math.floor(Math.random()*9)+1;		num2 = Math.floor(Math.random()*9)+1;		}		textBox = num1 + "x" + num2;		}						if (level == 4) {		if (randomz >  0.66) {		num2 = Math.floor(Math.random()*9)+1;		num1 = num2*total;		trace(num1 + "/" + num2 + "=" + total);		trace("correct answer");		} else {		num2 = Math.floor(Math.random()*9)+1;		randomz = Math.floor(Math.random()*9)+1;		num1 = randomz*num2;		}		textBox = num1 + "/" + num2;		}								return textBox;		}						public function goDie ():void {				}	}}